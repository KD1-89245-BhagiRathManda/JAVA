package com.sunbeam;

class Box{
	private Object obj;

	public Object getObj() {
		return obj;
	}

	public void setObj(Object obj) {
		this.obj = obj;
	}
	
}

public class BoxGen {

	public static void main(String[] args) {
		Box b = new Box();
		
		b.setObj(100);
		System.out.println();
		b.setObj(10.0);
		b.setObj("10");
	}

}
